{
  "name": "@liyuanqiu/final-state",
  "version": "1.0.4",
  "main": "dist/cjs/index.min.js",
  "module": "dist/es/index.min.js",
  "typings": "dist/cjs/index.d.ts",
  "author": "yuanqiulee@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/final-state/final-state"
  },
  "homepage": "https://final-state.github.io/final-state",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext ts src",
    "build": "yarn lint && yarn tsc-cjs && yarn minify-cjs && yarn tsc-es && yarn minify-es",
    "tsc-cjs": "tsc -p tsconfig.cjs.json",
    "tsc-es": "tsc -p tsconfig.es.json",
    "minify-cjs": "terser --compress --mangle --source-map --output dist/cjs/index.min.js -- dist/cjs/index.js",
    "minify-es": "terser --compress --mangle --source-map --output dist/es/index.min.js -- dist/es/index.js",
    "pub": "bash scripts/pub.sh",
    "test": "jest --coverage",
    "update-readme": "node scripts/update-readme.js"
  },
  "sideEffects": false,
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-jest": "^24.7.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.0",
    "immer": "^3.1.2",
    "jest": "^24.7.1",
    "prettier": "^1.17.1",
    "rxjs": "^6.5.2",
    "terser": "^4.0.0",
    "typescript": "^3.4.4"
  },
  "peerDependencies": {
    "immer": "^3.0.0"
  },
  "files": [
    "dist"
  ]
}
